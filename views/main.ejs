<%- include('head'); %>
<body>
	<%- include('navbar') %>
	<h1>Matches</h1>
	<div class="container">
		<%- include('banner') %>
		<div class="quidditch-logo"><img class="quidditch-logo-img" src="../images/quidditch-logo2.png" alt="Quidditch Logo" /></div>
		<main class="table-rows">
			<% meme.matches.forEach((match) => { %>
			<div class="match">
				<div class="team">
					<img src="../images/<%= match[0].image %>" alt="<% match[0].teamName %>" class="logo" />
					<div class="team-name"><%= match[0].teamName %></div>
				</div>

				<div class="versus">
					<img class="versus" src="/images/broom_versus_white.png" alt="Versus sign" />
				</div>

				<div class="team">
					<img src="../images/<%= match[1].image %>" alt="<% match[1].teamName %>" class="logo" />
					<div class="team-name"><%= match[1].teamName %></div>
				</div>
			</div>

			<% }) %>
		</main>
	</div>
	<%- include('footer'); %>

