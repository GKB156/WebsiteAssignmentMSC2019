<%- include('head'); %>
<body>
    <%- include('navbar', { loggedIn: loggedIn } ) %>
    <h1>Matches</h1>

<body>
    <%- include('navbar') %>
    </div>
        <h2>This weeks games - login or register to play</h2>
    </div>
    <ul>
        <% meme.forEach((game) => { %>
            <% game.matches.forEach((match) => { %>
                <div class="row">
                    <!-- First row -->
                    <!-- Left match -->
                    <div class="left-teams">
                      <div class="team">
                        <img  src="../images/<%= match[0].image %>" alt="" style="width:80%" class="logo">
                        <div class="team-name"><%= match[0].teamName %></div>
                      </div>                
                      <div class="versus">
                        V
                      </div>
                      <div class="team">
                        <img src="../images/<%= match[1].image %>" alt="" style="width:80%" class="logo">
                        <div class="team-name"><%= match[1].teamName %></div>
                      </div>
                    </div>
                    </div>
                  </div>
            <% }) %>
        <% }) %>
    </ul>
<%- include('footer'); %>